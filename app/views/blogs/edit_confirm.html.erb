<!--edit_confirm-->
<%= render "confirm" %>

<%= form_with(model: @blog, local: true, url: edit_blog_path, method: :get) do |form| %>
  <%= form.hidden_field :title %>
  <%= form.hidden_field :content %>
  <div class="confirm-back-btn">
    <ul class="edit-menu col-md-2 col-sm-12">
      <li><%= form.submit "Back." , name: "back", id: 'btn' %></li>
    </ul>
  </div>
<% end %>
